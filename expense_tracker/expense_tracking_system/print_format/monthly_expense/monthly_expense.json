 "html": "<!DOCTYPE html>\r\n<html>\r\n<head>\r\n    <style>\r\n        body {\r\n            font-family: Arial, sans-serif;\r\n            margin: 20px;\r\n        }\r\n        .header {\r\n            text-align: center;\r\n            font-size: 18px;\r\n            font-weight: bold;\r\n            margin-bottom: 20px;\r\n        }\r\n        .table {\r\n            width: 100%;\r\n            border-collapse: collapse;\r\n        }\r\n        .table th, .table td {\r\n            border: 1px solid #000;\r\n            padding: 8px;\r\n            text-align: left;\r\n        }\r\n    </style>\r\n</head>\r\n<body>\r\n    <div class=\"header\">\r\n        Monthly Expense Summary: {{ doc.month }} {{ doc.year }}\r\n    </div>\r\n\r\n    <table class=\"table\">\r\n        <tr>\r\n            <th>Vendor</th>\r\n            <th>Expense Date</th>\r\n            <th>Category</th>\r\n            <th>Amount</th>\r\n        </tr>\r\n        {% for expense in doc.expenses %}\r\n        <tr>\r\n            <td>{{ expense.vendor }}</td>\r\n            <td>{{ expense.expense_date }}</td>\r\n            <td>{{ expense.category }}</td>\r\n            <td>{{ expense.amount }}</td>\r\n        </tr>\r\n        {% endfor %}\r\n    </table>\r\n\r\n    <div class=\"footer\">\r\n        <p>Total Expenses: {{ doc.total_expense }}</p>\r\n    </div>\r\n</body>\r\n</html>\r\n",
